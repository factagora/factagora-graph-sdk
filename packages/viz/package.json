{
  "name": "@factagora/viz",
  "version": "0.1.0",
  "description": "Factagora 그래프/타임라인 시각화 컴포넌트 (React + tree-shaking)",
  "keywords": [
    "factagora",
    "visualization",
    "graph",
    "timeline",
    "react",
    "force-graph",
    "vis-timeline"
  ],
  "author": "Factagora Team",
  "license": "MIT",
  "packageManager": "pnpm@10.30.2",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./graph": {
      "import": {
        "types": "./dist/graph.d.ts",
        "default": "./dist/graph.js"
      },
      "require": {
        "types": "./dist/graph.d.cts",
        "default": "./dist/graph.cjs"
      }
    },
    "./timeline": {
      "import": {
        "types": "./dist/timeline.d.ts",
        "default": "./dist/timeline.js"
      },
      "require": {
        "types": "./dist/timeline.d.cts",
        "default": "./dist/timeline.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@factagora/types": "workspace:*",
    "@dagrejs/dagre": "^1.1.5"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "lucide-react": "^0.460.0",
    "@xyflow/react": "^12.0.0",
    "react-force-graph-2d": "^1.25.0",
    "zustand": "^5.0.0 || ^4.0.0"
  },
  "devDependencies": {
    "@factagora/tsconfig": "workspace:*",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tsup": "^8.3.5",
    "typescript": "^5.9.3"
  },
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/factagora/factagora-graph-sdk.git",
    "directory": "packages/viz"
  }
}
